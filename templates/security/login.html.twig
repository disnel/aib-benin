<body class="hold-transition login-page">
<div class="login-box">
<div class="login-logo">
<a href="#"><b>Administration</b></a>
</div>

<div class="login-box-body">
<p class="login-box-msg">Connexion à la page d'administration</p>
<form  method="post">

    {% if error %}
        <div class="alert alert-danger text-center">{{ error.messageKey|trans(error.messageData, 'security') }}</div>
    {% endif %}

    <div class="form-group has-feedback">
        <input type="text" id="username" class="form-control" name="_username" value="{{ last_username }}" placeholder="identifiant">
        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
    </div>
    <div class="form-group has-feedback">
        <input type="password" class="form-control" required="required" id="password" name="_password" autocomplete="off">
        <input type="hidden" name="_csrf_token" value="{{ csrf_token('authenticate') }}">
        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
    </div>
    <div class="row">

        <div class="col-xs-8">
            <div class="checkbox icheck">
                <label>
                <input type="checkbox"> Se souvenir de moi
                </label>
            </div>
        </div>

        <div class="col-xs-4">
            <button type="submit" class="btn btn-primary btn-block btn-flat">Connexion</button>
        </div>

    </div>
</form>
<div class="social-auth-links text-center">
    <p>- OU -</p>
    <a href="#" class="btn btn-block btn-social btn-facebook btn-flat"><i class="fa fa-facebook"></i> Connectez vous en utilisant Facebook</a>
    <a href="#" class="btn btn-block btn-social btn-google btn-flat"><i class="fa fa-google-plus"></i> Connecter vous en utilisant Google+</a>
</div>

    <a href="#">Mot de passe oublié</a><br>
</div>

</div>


